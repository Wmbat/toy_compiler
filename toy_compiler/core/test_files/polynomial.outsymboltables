symbol_table(
	name=translation unit, 
	values=symbol(name=main, kind=main, type=, link=Yes)
	       symbol(name=QUADRATIC, kind=class, type=, link=Yes)
	       symbol(name=LINEAR, kind=class, type=, link=Yes)
	       symbol(name=POLYNOMIAL, kind=class, type=, link=Yes))

symbol_table(
	name=main, 
	values=symbol(name=counter, kind=variable, type=integer, link=No)
	       symbol(name=f2, kind=variable, type=quadratic, link=No)
	       symbol(name=f1, kind=variable, type=linear, link=No))

